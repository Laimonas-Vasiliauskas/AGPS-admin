<?xml version="1.0" encoding="utf-8" ?>
<configuration>

	<connectionStrings>
		<add name="AGPSdb"
			 connectionString="Server=tcp:agpsserver.database.windows.net,1433;Initial Catalog=AGPSdb;Persist Security Info=False;User ID=AGPSadmin;Password={PWD};MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
